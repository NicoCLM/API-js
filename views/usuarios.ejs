<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Usuarios</title>
</head>
<body>
    <h1>Lista de Usuarios</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre</th>
            </tr>
        </thead>
        <tbody>
            <% usuarios.forEach(usuario => { %>
                <tr>
                    <td><%= usuario.id_usuario %></td>
                    <td><%= usuario.nombre %></td>
                    <td><%= usuario.email %></td>
                    <td><%= usuario.clave %></td>
                    <td><%= usuario.ciudad %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>